{
  "name": "scheduling-app",
  "version": "1.0.0",
  "description": "Jarett Lee mentored by Akjil Agrawal",
  "main": "index.js",
  "scripts": {
    "build-css": "stylus source/stylesheets/index.styl -o static/css",
    "watch-css": "stylus source/stylesheets/index.styl -o static/css -w",
    "clean": "rm -rf static/css && mkdir -p static/css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon server -e js,jade",
    "start": "node server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Owloid/scheduling-app.git"
  },
  "author": "Jarett Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Owloid/scheduling-app/issues"
  },
  "homepage": "https://github.com/Owloid/scheduling-app#readme",
  "dependencies": {
    "angular": "^1.5.8",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "firebase": "^3.5.1",
    "google-auth-library": "^0.9.9",
    "google-calendar": "^1.3.2",
    "googleapis": "^14.1.0",
    "method-override": "^2.3.6"
  }
}
